ad:adres_tbl a owl:Class ;
    rdfs:label "adres_tbl" ;
    ad:herkomstDefinitie "PNH" ;
    ad:isBGT_Object "Nee" ;
    ad:positioneleNauwkeurigheid "None" ;
    rdfs:subClassOf ad:areaaldata,
        bs:SpatialRegion ;
    skos:definition "Een object met adresgegevens, om naar te verwijzen vanuit kunstwerken, vri_; en ovl_kasten" .

ad:bak_p a owl:Class ;
    rdfs:label "bak_p" ;
    ad:herkomstDefinitie "PNH" ;
    ad:isBGT_Object "Nee" ;
    ad:positioneleNauwkeurigheid "None" ;
    rdfs:subClassOf ad:areaaldata,
        bs:PhysicalObject ;
    skos:definition "Object met een permanent karakter dat dient om iets in op te bergen of te verzamelen." .

ad:begroeidTerreindeelBermKr_l a owl:Class ;
    rdfs:label "begroeidTerreindeelBermKr_l" ;
    ad:herkomstDefinitie "BGT" ;
    ad:isBGT_Object "Ja" ;
    ad:positioneleNauwkeurigheid "None" ;
    rdfs:subClassOf ad:areaaldata,
        bs:SpatialRegion ;
    skos:definition "Lijngeometrie van de hoogstgelegen begrenzing van een kunstmatig aangelegd en onderhouden helling. Let op: Kruinlijnen zijn gerelateerd aan hun respectievelijke objecten middels een relatie op IDENTIFICATIE met IDENTIFICATIE" .

ad:begroeidTerreindeelBerm_v a owl:Class ;
    rdfs:label "begroeidTerreindeelBerm_v" ;
    ad:herkomstDefinitie "PNH" ;
    ad:isBGT_Object "Ja" ;
    ad:positioneleNauwkeurigheid "None" ;
    rdfs:subClassOf ad:areaaldata,
        bs:PhysicalObject ;
    skos:definition "Een ecologische berm met een vegetatie van grassen en kruiden. BGT Begroeid Terreindeel, verbijzonderd naar fysiek voorkomen groenvoorziening: gras- en kruidachtigen. Deze berm ligt verder dan 3 meter van een provinciale weg. Bermen dichter dan 3 meter bij een provinciale weg worden vastgelegd in ondersteunendWegdeel_v." .

