# baseURI: http://otl.noord-holland.nl/doc/v0.1/
# imports: http://purl.org/dc/terms/
# imports: http://www.opengis.net/ont/geosparql
# imports: http://www.w3.org/2004/02/skos/core

@prefix : <http://otl.noord-holland.nl/doc/v0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:
  rdf:type owl:Ontology ;
  owl:imports <http://purl.org/dc/terms/> ;
  owl:imports <http://www.opengis.net/ont/geosparql> ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
  owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
.
:DekkingGeografischGebied
  rdf:type owl:Class ;
  rdfs:label "DekkingGeografischGebied"^^xsd:string ;
  rdfs:subClassOf :InformatieOmschrijving ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass <http://www.opengis.net/ont/sf#Geometry> ;
      owl:onProperty <http://www.opengis.net/ont/geosparql#hasGeometry> ;
    ] ;
.
:DekkingInTijd
  rdf:type owl:Class ;
  rdfs:label "DekkingInTijd"^^xsd:string ;
  rdfs:subClassOf :InformatieOmschrijving ;
.
:Document
  rdf:type owl:Class ;
  rdfs:label "Document"^^xsd:string ;
  rdfs:subClassOf :InformatieObject ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty :documentheeftnaam ;
    ] ;
.
:DocumentNaam
  rdf:type owl:Class ;
  rdfs:label "DocumentNaam"^^xsd:string ;
  rdfs:subClassOf :InformatieOmschrijving ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty :onderwerp ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty :soortdocument ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty :versie ;
    ] ;
.
:DocumentReden
  rdf:type owl:Class ;
  rdfs:label "DocumentReden"^^xsd:string ;
  rdfs:subClassOf :InformatieOmschrijving ;
.
:DocumentSoort
  rdf:type owl:Class ;
  rdfs:label "DocumentSoort"^^xsd:string ;
  rdfs:subClassOf :InformatieOmschrijving ;
.
:Geo-object
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:domain :DekkingGeografischGebied ;
  rdfs:label "Geo-object"^^xsd:string ;
  rdfs:range xsd:string ;
.
:Handeling
  rdf:type owl:Class ;
  rdfs:label "Handeling"^^xsd:string ;
  rdfs:subClassOf :InformatieOmschrijving ;
.
:InformatieObject
  rdf:type owl:Class ;
  rdfs:label "InformatieObject"^^xsd:string ;
  rdfs:subClassOf owl:Thing ;
.
:InformatieOmschrijving
  rdf:type owl:Class ;
  rdfs:label "InformatieOmschrijving"^^xsd:string ;
  rdfs:subClassOf owl:Thing ;
.
:Ontvangen
  rdf:type :DocumentReden ;
  rdfs:label "Ontvangen"^^xsd:string ;
.
:Opgemaakt
  rdf:type :DocumentReden ;
  rdfs:label "Opgemaakt"^^xsd:string ;
.
:Project
  rdf:type owl:Class ;
  rdfs:label "Project"^^xsd:string ;
  rdfs:subClassOf :InformatieObject ;
.
:Verzonden
  rdf:type :DocumentReden ;
  rdfs:label "Verzonden"^^xsd:string ;
.
:Zaak
  rdf:type owl:Class ;
  rdfs:label "Zaak"^^xsd:string ;
  rdfs:subClassOf :InformatieObject ;
.
:documentheeftdekkinggeografischgebied
  rdf:type rdf:Property ;
  rdfs:domain :Document ;
  rdfs:label "documentheeftdekkinggeografischgebied"^^xsd:string ;
  rdfs:range :DekkingGeografischGebied ;
.
:documentheeftdekkingintijd
  rdf:type rdf:Property ;
  rdfs:domain :Document ;
  rdfs:label "documentheeftdekkingintijd"^^xsd:string ;
  rdfs:range :DekkingInTijd ;
.
:documentheeftnaam
  rdf:type owl:FunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Document ;
  rdfs:label "Document heeft Naam"^^xsd:string ;
  rdfs:range :DocumentNaam ;
.
:documentheeftreden
  rdf:type owl:FunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Document ;
  rdfs:label "Document heeft Reden"^^xsd:string ;
  rdfs:range :DocumentReden ;
.
:documentheeftsoort
  rdf:type owl:FunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Document ;
  rdfs:label "Document heeft Soort"^^xsd:string ;
  rdfs:range :DocumentSoort ;
.
:documenthoortbijzaak
  rdf:type owl:FunctionalProperty ;
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Document ;
  rdfs:label "Document hoort bij Zaak"^^xsd:string ;
  rdfs:range :Zaak ;
.
:onderwerp
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:FunctionalProperty ;
  rdfs:domain :DocumentNaam ;
  rdfs:label "onderwerp"^^xsd:string ;
  rdfs:range xsd:string ;
.
:periode
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:FunctionalProperty ;
  rdfs:domain :Document ;
  rdfs:label "periode"^^xsd:string ;
  rdfs:range xsd:date ;
.
:soortdocument
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:FunctionalProperty ;
  rdfs:domain :DocumentNaam ;
  rdfs:label "soortdocument"^^xsd:string ;
  rdfs:range xsd:string ;
.
:versie
  rdf:type owl:DatatypeProperty ;
  rdf:type owl:FunctionalProperty ;
  rdfs:domain :DocumentNaam ;
  rdfs:label "versie"^^xsd:string ;
  rdfs:range xsd:string ;
.
