@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

doc:
  rdf:type owl:Ontology ;
  owl:imports <http://purl.org/dc/terms/> ;
  owl:imports <http://www.opengis.net/ont/geosparql> ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
  owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
.
doc:Bestek
  rdf:type doc:DocumentType ;
  rdfs:label "Bestek"^^xsd:string ;
.
doc:DekkingGeografischGebied
  rdf:type owl:Class ;
  rdfs:label "DekkingGeografischGebied"^^xsd:string ;
  rdfs:subClassOf doc:InformatieOmschrijving ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass <http://www.opengis.net/ont/sf#Geometry> ;
      owl:onProperty <http://www.opengis.net/ont/geosparql#hasGeometry> ;
    ] ;
.
doc:DekkingInTijd
  rdf:type owl:Class ;
  rdfs:label "DekkingInTijd"^^xsd:string ;
