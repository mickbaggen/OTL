ad:begroeidTerreindeelBermKr_l a owl:Class ;
    rdfs:label "begroeidTerreindeelBermKr_l" ;
    ad:herkomstDefinitie "BGT" ;
    ad:isBGT_Object "Ja" ;
    ad:positioneleNauwkeurigheid "afgeleid van object waar kruinlijn bij hoort" ;
    rdfs:subClassOf ad:areaaldata ;
    skos:definition "Lijngeometrie van de hoogstgelegen begrenzing van een kunstmatig aangelegd en onderhouden helling. Let op: Kruinlijnen zijn gerelateerd aan hun respectievelijke objecten middels een relatie op IDENTIFICATIE met IDENTIFICATIE" .

ad:begroeidTerreindeelBerm_v a owl:Class ;
