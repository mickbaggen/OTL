adkern:aanduidingEisVoorzorgsmaatr_p a owl:Class ;
    rdfs:label "aanduidingEisVoorzorgsmaatr_p" ;
    adkern:herkomstDefinitie "IMKL (2015)" ;
    adkern:isBGT_Object "Nee" ;
    adkern:positioneleNauwkeurigheid "7,5 cm" ;
    rdfs:subClassOf adkern:areaaldata ;
    skos:definition "Een eis voorzorgsmaatregel geeft aan dat de beheerder van een net aangegeven heeft dat er sprake is van voorzorgsmaatregelen die getroffen dienen te worden. Dit betreft alleen de wettelijk geregelde eis voorzorgsmaatregel. (Bron: IMKL 2015)" .

adkern:aanduidingVerzoekContact_p a owl:Class ;
    rdfs:label "aanduidingVerzoekContact_p" ;
    adkern:herkomstDefinitie "IMKL (2015)" ;
    adkern:isBGT_Object "Nee" ;
    adkern:positioneleNauwkeurigheid "n.v.t." ;
    rdfs:subClassOf adkern:areaaldata ;
    skos:definition "Aanduiding verzoek contact" .

adkern:adres_tbl a owl:Class ;
    rdfs:label "adres_tbl" ;
    adkern:herkomstDefinitie "PNH" ;
    adkern:isBGT_Object "Nee" ;
    adkern:positioneleNauwkeurigheid "nvt" ;
    rdfs:subClassOf adkern:areaaldata ;
    skos:definition "Een object met adresgegevens, om naar te verwijzen vanuit kunstwerken, vri_- en ovl_kasten" .

adkern:bak_p a owl:Class ;
    rdfs:label "bak_p" ;
    adkern:herkomstDefinitie "PNH" ;
    adkern:isBGT_Object "Nee" ;
    adkern:positioneleNauwkeurigheid "15 cm" ;
    rdfs:subClassOf adkern:areaaldata ;
    skos:definition "Object met een permanent karakter dat dient om iets in op te bergen of te verzamelen." .

adkern:begroeidTerreindeelBerm_v a owl:Class ;
    rdfs:label "begroeidTerreindeelBerm_v" ;
    adkern:herkomstDefinitie "PNH" ;
    adkern:isBGT_Object "Ja" ;
    adkern:positioneleNauwkeurigheid "25 cm" ;
    rdfs:subClassOf adkern:areaaldata ;
    skos:definition "Een ecologische berm met een vegetatie van grassen en kruiden. BGT Begroeid Terreindeel, verbijzonderd naar fysiek voorkomen groenvoorziening: gras- en kruidachtigen." .

adkern:begroeidTerreindeelKruin_l a owl:Class ;
    rdfs:label "begroeidTerreindeelKruin_l" ;
    adkern:herkomstDefinitie "BGT" ;
    adkern:isBGT_Object "Ja" ;
    adkern:positioneleNauwkeurigheid "afgeleid van object waar kruinlijn bij hoort" ;
    rdfs:subClassOf adkern:areaaldata ;
    skos:definition "Lijngeometrie van de hoogstgelegen begrenzing van een kunstmatig aangelegd en onderhouden helling." .

adkern:begroeidTerreindeelPlantvak_v a owl:Class ;
    rdfs:label "begroeidTerreindeelPlantvak_v" ;
    adkern:herkomstDefinitie "PNH" ;
    adkern:isBGT_Object "Ja" ;
    adkern:positioneleNauwkeurigheid "25 cm" ;
    rdfs:subClassOf adkern:areaaldata ;
    skos:definition "Terreindeel met aangelegde beplanting, meestal gras, heesters of struiken. BGT Begroeid Terreindeel, verbijzonderd naar fysiek voorkomen groenvoorziening." .

adkern:begroeidTerreindeel_v a owl:Class ;
    rdfs:label "begroeidTerreindeel_v" ;
    adkern:herkomstDefinitie "BGT/PNH" ;
    adkern:isBGT_Object "Ja" ;
    adkern:positioneleNauwkeurigheid "25 cm" ;
    rdfs:subClassOf adkern:areaaldata ;
    skos:definition "Kleinste functioneel onafhankelijk stukje van een terrein dat er binnen het objecttype Terrein van NEN 3610 wordt onderscheiden, met aaneengesloten vegetatie. Minus de specifieke objecten die PNH geadministreerd wil hebben in begroeidTerreindeelBerm_v en begroeidTerreindeelPlantvak_v" .

adkern:beschoeiingInspectie_tbl a owl:Class ;
    rdfs:label "beschoeiingInspectie_tbl" ;
    adkern:herkomstDefinitie "PNH" ;
    adkern:isBGT_Object "Nee" ;
    adkern:positioneleNauwkeurigheid "nvt" ;
    rdfs:subClassOf adkern:areaaldata ;
    skos:definition "Inspectieobject met alle inspectiegegevens die ingewonnen zijn m.b.t. beschoeiingen" .

adkern:bodem_v a owl:Class ;
    rdfs:label "bodem_v" ;
    adkern:herkomstDefinitie "PNH" ;
    adkern:isBGT_Object "Nee" ;
    adkern:positioneleNauwkeurigheid "25 cm" ;
    rdfs:subClassOf adkern:areaaldata ;
    skos:definition "Bodem van een waterdeel, hier worden de gegevens bijgehouden die van belang zijn voor o.a. baggeren" .

adkern:bordScheepvaart_p a owl:Class ;
    rdfs:label "bordScheepvaart_p" ;
    adkern:herkomstDefinitie "PNH/IMGEO" ;
    adkern:isBGT_Object "Nee" ;
    adkern:positioneleNauwkeurigheid "15 cm" ;
    rdfs:subClassOf adkern:areaaldata ;
    skos:definition "Een paneel waarop informatie wordt afgebeeld, specifiek bedoelt voor de scheepvaart." .

adkern:bordWegwijzer_p a owl:Class ;
    rdfs:label "bordWegwijzer_p" ;
    adkern:herkomstDefinitie "PNH/IMGEO" ;
    adkern:isBGT_Object "Nee" ;
    adkern:positioneleNauwkeurigheid "15 cm" ;
    rdfs:subClassOf adkern:areaaldata ;
    skos:definition "Constructie voorzien van een of meer panelen met informatie ten behoeve van de bewegwijzering. Let op: dit" .

adkern:bordZwemwater_p a owl:Class ;
    rdfs:label "bordZwemwater_p" ;
    adkern:herkomstDefinitie "PNH/IMGEO" ;
    adkern:isBGT_Object "Nee" ;
    adkern:positioneleNauwkeurigheid "15 cm" ;
    rdfs:subClassOf adkern:areaaldata ;
    skos:definition "Een paneel waarop informatie wordt afgebeeld ten behoeve van zwemwater locaties." .

adkern:bord_p a owl:Class ;
    rdfs:label "bord_p" ;
    adkern:herkomstDefinitie "PNH/IMGEO" ;
    adkern:isBGT_Object "Nee" ;
    adkern:positioneleNauwkeurigheid "15 cm" ;
    rdfs:subClassOf adkern:areaaldata ;
    skos:definition "Een paneel waarop informatie wordt afgebeeld. Dit zijn zowel RVV borden als bijvoorbeeld DRIS Panelen (voor OV reizigers) en matrix borden (voor automobilisten) - DRIP panelen zijn in Areaaldata te vinden als matrix borden met een TOPdesk ID" .

adkern:bouwdeelGeluidsscherm_tbl a owl:Class ;
    rdfs:label "bouwdeelGeluidsscherm_tbl" ;
    adkern:herkomstDefinitie "NEN 2767-4 / PNH" ;
    adkern:isBGT_Object "Nee" ;
    adkern:positioneleNauwkeurigheid "nvt" ;
    rdfs:subClassOf adkern:areaaldata ;
    skos:definition "Laagste niveau van decompositie van geluidwerende voorziening via geluidwerende constructie. Het begrip NEN Bouwdeel is niet gedefinieerd." .

adkern:bouwdeelKunstwerk_tbl a owl:Class ;
    rdfs:label "bouwdeelKunstwerk_tbl" ;
    adkern:herkomstDefinitie "NEN 2767-4 / PNH" ;
    adkern:isBGT_Object "Nee" ;
    adkern:positioneleNauwkeurigheid "nvt" ;
    rdfs:subClassOf adkern:areaaldata ;
    skos:definition "Laagste niveau van decompositie van kunstwerk_vast via kwElement. Het begrip NEN Bouwdeel is niet gedefinieerd." .

