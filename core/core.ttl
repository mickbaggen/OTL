# baseURI: http://otl.noord-holland.nl/core/
# imports: http://binkhoffmann.org/ontologies/top
# imports: http://www.w3.org/2004/02/skos/core
# prefix: core

@prefix core: <http://otl.noord-holland.nl/core/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

core:
  rdf:type owl:Ontology ;
  owl:imports <http://binkhoffmann.org/ontologies/top> ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
core:As
  rdf:type owl:Class ;
  rdfs:subClassOf <http://binkhoffmann.org/ontologies/top#EenDRuimte> ;
.
core:AsGeometrie
  rdf:type owl:Class ;
  rdfs:subClassOf <http://binkhoffmann.org/ontologies/top#EenDCoordinaat> ;
.
core:Baan
  rdf:type owl:Class ;
  rdfs:label "Baan"@nl-nl ;
  rdfs:subClassOf <http://binkhoffmann.org/ontologies/top#Artefact> ;
  skos:definition "Gedeelte van een weg, in lengterichting begrensd door een beginraai en een eindraai, en in dwarsrichting begrensd door:  twee opeenvolgende overgangen verhard/onverhard of een overgang verhard/onverhard en een weggrens;"@nl-nl ;
.
core:BaanAs
  rdf:type owl:Class ;
  rdfs:subClassOf core:WegAs ;
.
core:Constructie
  rdf:type owl:Class ;
  rdfs:label "Constructie"@nl-nl ;
  rdfs:subClassOf <http://binkhoffmann.org/ontologies/top#Artefact> ;
.
core:FietsNetwerk
  rdf:type owl:Class ;
  rdfs:subClassOf core:Netwerk ;
.
core:GeleiderailConstructie
  rdf:type owl:Class ;
  rdfs:label "Geleiderail constructie" ;
  rdfs:subClassOf core:ScheidendeConstructie ;
.
core:GeluidbeperkendeConstructie
  rdf:type owl:Class ;
  rdfs:subClassOf core:ScheidendeConstructie ;
  skos:definition "Samenstel van meerdere constructie delen dat een bepaald doel dient, en dat het resultaat is van een constructieproces"@nl-nl ;
.
core:Halte
  rdf:type owl:Class ;
