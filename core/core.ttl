  rdfs:subClassOf core:Constructie ;
.
core:StrookAs
  rdf:type owl:Class ;
  rdfs:subClassOf core:WegAs ;
.
core:Traject
  rdf:type owl:Class ;
  rdfs:subClassOf <http://binkhoffmann.org/ontologies/top#DrieDRuimte> ;
  skos:definition "Fysieke ruimte die een eenduidig afgebakende en samenhangende deelverzameling vormt in de topologie van het netwerk."@nl-nl ;
  skos:scopeNote "Richtlijnen voor omvang van een wegtraject: + Een traject start/eindigt bij een eigendoms/beheergrens + Een traject start/eindigt bij een aansluiting op een weg van een hogere orde. + Een traject start/eindigt op een weg van een gelijke orde." ;
.
core:UitwisselpuntVaarweg
  rdf:type owl:Class ;
  rdfs:subClassOf core:VaarwegNode ;
.
