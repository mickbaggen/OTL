.
core:VaarwegNetwerk
  rdf:type owl:Class ;
  rdfs:subClassOf core:Netwerk ;
.
core:VaarwegNode
  rdf:type owl:Class ;
  rdfs:subClassOf core:NetwerkNode ;
.
core:VerblijfspuntVaarweg
  rdf:type owl:Class ;
  rdfs:subClassOf core:VaarwegNode ;
.
core:VerblijfspuntWeg
  rdf:type owl:Class ;
  rdfs:subClassOf core:WegNode ;
.
core:VerhardeBaan
  rdf:type owl:Class ;
  rdfs:label "Verharde baan"@nl-nl ;
  rdfs:subClassOf core:Baan ;
  skos:definition "Baan die een onderdeel vormt van een verhardingsoppervlak."@nl-nl ;
.
core:Vluchthaven
  rdf:type owl:Class ;
  rdfs:subClassOf core:VerblijfspuntWeg ;
.
core:Wachtplaats
  rdf:type owl:Class ;
  rdfs:subClassOf core:VerblijfspuntVaarweg ;
.
core:WegAs
  rdf:type owl:Class ;
  rdfs:subClassOf core:As ;
.
core:WegBaan
  rdf:type owl:Class ;
  rdfs:subClassOf core:WegLink ;
.
core:WegLink
  rdf:type owl:Class ;
  rdfs:subClassOf core:NetwerkLink ;
.
core:WegNetwerk
  rdf:type owl:Class ;
  rdfs:subClassOf core:Netwerk ;
