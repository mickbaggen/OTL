.
core:Netwerk
  rdf:type owl:Class ;
  rdfs:label "Netwerk" ;
  rdfs:subClassOf <http://binkhoffmann.org/ontologies/top#Systeem> ;
  skos:definition "Samenstelling van netwerkelementen, waaronder nodes, links en kruisingen."@nl-NL ;
.
core:NetwerkKruising
  rdf:type owl:Class ;
  rdfs:label "Netwerk kruising" ;
  rdfs:subClassOf <http://binkhoffmann.org/ontologies/top#DrieDRuimte> ;
  skos:definition "Locatie, waar een of meer netwerklinks elkaar ontmoeten, maar waar geen uitwisseling mogelijk is."@nl-NL ;
.
core:NetwerkLink
  rdf:type owl:Class ;
  rdfs:label "Netwerk link" ;
  rdfs:subClassOf <http://binkhoffmann.org/ontologies/top#DrieDRuimte> ;
.
core:NetwerkNode
  rdf:type owl:Class ;
  rdfs:label "Netwerk node" ;
  rdfs:subClassOf <http://binkhoffmann.org/ontologies/top#DrieDRuimte> ;
.
core:Oeverconstructie
  rdf:type owl:Class ;
  rdfs:subClassOf core:ScheidendeConstructie ;
  skos:definition """Een Oeverconstructie heeft als functie het achterliggende land te beschermen tegen het
water."""@nl-nl ;
.
core:Oeverlijn
  rdf:type owl:Class ;
  rdfs:subClassOf <http://binkhoffmann.org/ontologies/top#EenDRuimte> ;
.
core:Oevervak
  rdf:type owl:Class ;
  rdfs:subClassOf <http://binkhoffmann.org/ontologies/top#DrieDRuimte> ;
  skos:definition "Een Oevervak is de rand van een kanaal, vaarweg of rivier. Het begint bij bodem van het nautisch profiel en gaat door tot 10m achter de oeverconstructie."@nl-nl ;
.
core:OpenbaarVervoerNetwerk
  rdf:type owl:Class ;
  rdfs:subClassOf core:Netwerk ;
.
core:Parkeerplaats
